CREATE TABLE UserEntity(
    id INTEGER NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    nickname TEXT NOT NULL,
    birth TEXT NOT NULL,
    phone TEXT NOT NULL,
    zodiac TEXT NOT NULL,
    about_me TEXT NOT NULL,
    city TEXT NOT NULL);


getUser: SELECT * FROM UserEntity;

insertUser:
INSERT OR REPLACE INTO UserEntity VALUES ?;

getUserById:
SELECT * FROM UserEntity WHERE id = :id;